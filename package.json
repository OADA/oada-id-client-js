{
  "name": "@oada/id-client",
  "version": "2.0.0",
  "description": "OADA identity client library for Node.JS and Browser",
  "main": "index.js",
  "browser": "src/browser.js",
  "unpkg": "dist/bundle.js",
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "index.js",
    "src/",
    "dist/"
  ],
  "scripts": {
    "prepublish": "npm run build && npm run bundle",
    "bundle": "webpack",
    "build": "tsc -b",
    "test": "cross-env TS_NODE_PROJECT='test/tsconfig.json' mocha",
    "test:debug": "yarn test -j 0 --full-trace -t 100000",
    "test:browser": "karma start --single-run",
    "test:cover": "cross-env TS_NODE_PROJECT='test/tsconfig.json' c8 mocha",
    "dev": "cpx --watch --verbose './' '../oada-cache/node_modules/oada-id-client'",
    "clean": "rm -rf ./coverage"
  },
  "babel": {
    "presets": [
      [
        "@babel/env",
        {
          "useBuiltIns": "entry"
        }
      ]
    ]
  },
  "browserslist": "> 0.25%, not dead",
  "c8": {
    "reporter": [
      "text",
      "lcov"
    ],
    "all": true,
    "src": "lib",
    "exclude": [
      "*.d.ts",
      ".pnp.*",
      ".test"
    ]
  },
  "directories": {
    "lib": "src",
    "test": "test",
    "example": "examples"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OADA/oada-id-client-js.git"
  },
  "keywords": [
    "oada",
    "browser",
    "openid",
    "oauth"
  ],
  "author": {
    "name": "Alex Layton",
    "email": "alex@layton.in",
    "url": "http://alex.layton.in"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/OADA/oada-id-client-js/issues"
  },
  "homepage": "https://github.com/OADA/oada-id-client-js",
  "dependencies": {
    "@oada/certs": "^4.1.1",
    "is-wsl": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "jwt-bearer-client-auth": "^2.0.1",
    "oauth-dyn-reg": "^2.0.0",
    "object-assign": "^4.1.1",
    "open": "^8.4.0",
    "pem-jwk": "^2.0.0",
    "superagent": "^7.1.2",
    "tslib": "^2.3.1",
    "urijs": "^1.19.10"
  },
  "devDependencies": {
    "@oada/types": "^1.8.1",
    "@tsconfig/node12": "^1.0.9",
    "@types/body-parser": "^1.19.2",
    "@types/chai": "^4.3.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mocha": "^9.1.0",
    "@types/node": "12.20.47",
    "@types/pem-jwk": "^2.0.0",
    "@types/rewire": "^2.5.28",
    "@types/sinon": "^10.0.11",
    "@types/superagent": "^4.1.15",
    "@types/urijs": "^1.19.19",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "@yarnpkg/sdks": "2.6.0",
    "async": "^3.2.3",
    "body-parser": "^1.19.2",
    "brfs": "^2.0.2",
    "c8": "^7.11.0",
    "chai": "^4.3.6",
    "coffee-script": "^1.12.7",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "crypto-browserify": "^3.12.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-xo": "^0.40.0",
    "eslint-config-xo-typescript": "^0.50.0",
    "eslint-formatter-pretty": "^4.1.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-plugin-array-func": "^3.1.7",
    "eslint-plugin-ava": "^13.2.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-github": "^4.3.6",
    "eslint-plugin-i18n-text": "^1.0.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-no-constructor-bind": "^2.0.4",
    "eslint-plugin-no-only-tests": "^2.6.0",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-notice": "^0.9.10",
    "eslint-plugin-optimize-regex": "^1.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-regexp": "^1.6.0",
    "eslint-plugin-security": "^1.4.0",
