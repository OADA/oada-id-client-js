    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-vivaldi-launcher": "0.0.1",
    "karma-webpack": "^5.0.0",
    "lodash": "^4.17.21",
    "mocha": "^9.2.2",
    "prettier": "^2.6.1",
    "process": "^0.11.10",
    "puppeteer": "^13.5.2",
    "rewire": "^6.0.0",
    "rewire-webpack-plugin": "^1.0.0",
    "sinon": "^13.0.1",
    "stream-browserify": "^3.0.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3",
    "url": "^0.11.0",
    "util": "^0.12.4",
    "wd": "^1.14.0",
    "webpack": "^5.71.0",
    "webpack-cli": "^4.9.2",
    "yargs": "^17.4.0"
  },
  "packageManager": "yarn@3.2.0"
}
