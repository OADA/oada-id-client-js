{
  "cSpell.words": [
    "brfs",
    "jwks",
    "jwku",
    "lcov",
    "middlewareify",
    "nodeify",
    "SCHAKLEFORD",
    "sonarjs",
    "urijs",
    "wslview"
  ],
  "files.exclude": {
    "*.log": true,
    "tags.*": true,
    "node_modules": true,
    ".vim": true,
    ".nvimlog": true,
    "dist": true,
    ".test": true,
    "coverage": true,
    ".history": true,
    ".dccache": true,
    ".yarn": true,
    ".pnp.*": true,
    "*.tsbuildinfo": true
  },
  "search.exclude": {
    "*.log": true,
    "tags.*": true,
    "node_modules": true,
    ".vim": true,
    ".nvimlog": true,
    "dist": true,
    ".test": true,
    "coverage": true,
    ".history": true,
    ".dccache": true,
    ".yarn": true,
    ".pnp.*": true,
    "*.tsbuildinfo": true,
    "**/.yarn": true,
    "**/.pnp.*": true
  },
  "eslint.nodePath": ".yarn/sdks",
  "prettier.prettierPath": ".yarn/sdks/prettier/index.js",
  "typescript.tsdk": ".yarn/sdks/typescript/lib",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "explorerExclude.backup": null,
  "npm.packageManager": "yarn",
  "debug.javascript.autoAttachFilter": "smart",
  "debug.javascript.breakOnConditionalError": true,
  "debug.javascript.defaultRuntimeExecutable": {
    "node": "yarn",
    "pwa-node": "yarn"
  },
  "debug.javascript.autoAttachSmartPattern": [
    "${workspaceFolder}/**",
    "!**/node_modules/**",
    "**/$KNOWN_TOOLS$/**",
    "!**/yarn-*",
    "!**/ava-*/**/cli.mjs"
  ]
}
